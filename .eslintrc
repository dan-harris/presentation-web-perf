---
"extends":
  - "formidable/configurations/es6-react"

"rules":
  "max-len": 0
  "no-magic-numbers": 0
  "react/prefer-es6-class": 0
  "react/no-multi-comp": 0
  "quotes": [2, "single"]
  "object-property-newline": 0
  "valid-jsdoc": 0
  "template-tag-spacing": 0
  "arrow-parens": 0
"env":
  "browser": true,
  "node": true
"globals":
  "afterEach": true,
  "describe": true,
  "expect": true,
  "it": true,
  "jest": true,
  "test": true
