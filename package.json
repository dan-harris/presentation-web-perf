{
  "name": "presentation-web-perf",
  "version": "2.3.0",
  "description": "danharris.io spectacle presentation boilerplate",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run clean & cross-env NODE_ENV=production webpack --config webpack.config.production.js && npm run replace",
    "clean": "rimraf dist",
    "deploy": "aws s3 sync ./dist s3://presentations.danharris.io/web-perf --exclude \"index.html\" --profile dan",
    "deploy:full": "aws s3 sync ./dist s3://presentations.danharris.io/web-perf --profile dan",
    "publish": "npm run build && npm run deploy",
    "export": "spectacle-renderer --delay 3000",
    "lint": "eslint --ext .js,.jsx .",
    "start": "cross-env NODE_ENV=development node server.js",
    "patch": "patch:spectacle && patch:spectacle-code-slide",
    "patch:spectacle": "patch-package spectacle",
    "patch:spectacle-code-slide": "patch-package spectacle-code-slide",
    "postinstall": "patch-package",
    "commit": "git-cz",
    "replace": "npm run replace:urls && npm run replace:https",
    "replace:urls": "replace-in-file localhost:3000 presentations.danharris.io dist/examples/*.html --verbose",
    "replace:https": "replace-in-file http://deelay.me https://deelay.me dist/examples/*.html --verbose"
  },
  "author": "Formidable Labs <hello@formidable.com> (https://formidable.com)",
  "contributors": [
    "danharris <me@danharris.io> (https://danharris.io)"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "normalize.css": "8.0.0",
    "prismjs": "^1.15.0",
    "react": "^16.5.2",
    "react-confetti": "^2.3.0",
    "react-dom": "^16.5.2",
    "react-iframe": "^1.8.0",
    "replace-in-file": "^4.0.0",
    "spectacle": "^5.2.2",
    "spectacle-code-slide": "^0.5.2",
    "spectacle-renderer": "^0.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0",
    "@babel/plugin-transform-react-inline-elements": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@emotion/core": "^10.0.6",
    "@emotion/styled": "^10.0.6",
    "@webpack-cli/migrate": "^0.1.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.18",
    "commitizen": "^3.0.5",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.6.1",
    "eslint-config-formidable": "^4.0.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "is-buffer": "^2.0.3",
    "markdown-loader": "^4.0.0",
    "node-libs-browser": "^2.1.0",
    "patch-package": "^6.0.2",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^4.3.11",
    "react-transform-catch-errors": "^1.0.2",
    "redbox-react": "^1.6.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.0",
    "surge": "latest",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^1.1.1",
    "webpack": "4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.9",
    "webpack-hot-middleware": "^2.24.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "helpMessage": "Use \"npm run commit\" instead, we use conventional-changelog format :) (https://github.com/commitizen/cz-cli)"
    }
  }
}
